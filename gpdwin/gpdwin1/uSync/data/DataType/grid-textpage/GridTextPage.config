      "label": "Background color",
      "description": "Choose background color",
      "key": "class",
      "view": "radiobuttonlist",
      "applyTo": "row",
      "prevalues": [
        "light",
        "dark",
        "yellow",
        "blue",
        "purple",
        "full",
        "full triangle"
      ]
    }
  ],
  "columns": 12,
  "templates": [
    {
      "name": "1 column layout",
      "sections": [
        {
          "grid": 12
        }
      ]
    }
  ],
  "layouts": [
    {
      "name": "Headline",
      "areas": [
        {
          "grid": 12,
          "editors": [
            "headline"
          ]
        }
      ]
    },
    {
      "name": "Article",
      "areas": [
        {
          "grid": 4
        },
        {
          "grid": 8
        }
      ]
    },
    {
      "name": "Wide image",
      "areas": [
        {
          "grid": 12,
          "allowAll": false,
          "allowed": [
            "media_wide",
            "media_wide_cropped"
          ]
        }
      ]
    },
    {
      "name": "Image w. text",
      "areas": [
        {
