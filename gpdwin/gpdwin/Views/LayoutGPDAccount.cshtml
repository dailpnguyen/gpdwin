@using ClientDependency.Core.Mvc
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    Html.RequiresCss("~/bootstrap/css/bootstrap.min.css");<!-- Bootstrap 3.3.6 -->
    Html.RequiresCss("https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css");<!-- Font Awesome -->
    Html.RequiresCss("https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css");<!-- Ionicons -->
    Html.RequiresCss("~/dist/css/AdminLTE.min.css"); <!-- Theme style -->
    Html.RequiresCss("~/plugins/iCheck/square/blue.css"); <!-- iCheck -->

    Html.RequiresJs("~/plugins/jQuery/jquery-2.2.3.min.js");<!-- jQuery 2.2.3 -->
    Html.RequiresJs("~/bootstrap/js/bootstrap.min.js");<!-- Bootstrap 3.3.6 -->
    Html.RequiresJs("~/plugins/iCheck/icheck.min.js");<!-- iCheck -->
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Log in</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    @Html.RenderCssHere()

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition @Umbraco.Field("cssClass")-page">
    <div class="@Umbraco.Field("cssClass")-box">
        <div class="@Umbraco.Field("cssClass")-logo">
            <a href="../../index2.html"><b>Admin</b>LTE</a>
        </div>
        <!-- /.login-logo -->
        <div class="@Umbraco.Field("cssClass")-box-body">
            @RenderBody()
        </div>
        <!-- /.login-box-body -->
    </div>

    @Html.RenderJsHere()
    <script>
    $(function () {
        $('input').iCheck({
            checkboxClass: 'icheckbox_square-blue',
            radioClass: 'iradio_square-blue',
            increaseArea: '20%' // optional
        });
    });
    </script>
</body>
</html>

